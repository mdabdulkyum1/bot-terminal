# Bot Terminal Configuration
# Choose your AI provider: 'gemini' or 'openai'
AI_PROVIDER=gemini

# Gemini Configuration (Google)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000

# Bot Terminal Settings
MAX_FILE_SIZE=1000000
ALLOWED_FILE_EXTENSIONS=.js,.ts,.jsx,.tsx,.py,.java,.cpp,.c,.h,.json,.md,.txt,.css,.html,.xml,.yaml,.yml
AUTO_SAVE_SESSIONS=true
SESSION_TIMEOUT=3600000

# Desktop App Settings
DESKTOP_WIDTH=1200
DESKTOP_HEIGHT=800
DESKTOP_TITLE=Bot Terminal - AI Development Assistant

# Development Settings
DEBUG_MODE=false
LOG_LEVEL=info
